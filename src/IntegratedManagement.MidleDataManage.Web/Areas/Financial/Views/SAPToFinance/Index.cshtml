<script>
    function onSyncData(servicename) {
        var lblName = "lbl" + servicename;
        var date = new Date();
        $.post('/Financial/SAPToFinance/SyncData', { ServiceName: servicename }, function (data) {
            $("#" + lblName).html(date.toLocaleString() + data.message);
        });
    }
</script>

